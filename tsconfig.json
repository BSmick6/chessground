{
  "include": ["src/*.ts"],
  "exclude": [],
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "noEmitOnError": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ES2017",
    "lib": ["DOM", "ES2017"]
  }
}
